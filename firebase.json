{
  "apphosting": {
    "backendId": "study-tracker-next",
    "rootDir": "/",
    "ignore": [
      "node_modules",
      ".git",
      "firebase-debug.log",
      "firebase-debug.*.log",
      "functions"
    ]
  },
  "emulators": {
    "firestore": {
      "port": 8080
    },
    "ui": {
      "enabled": true
    },
    "singleProjectMode": true,
    "import": "/src/emulators/data.ts",
    "exportOnExit": "/src/emulators/export.ts"
  }
}
